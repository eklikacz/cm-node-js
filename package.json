{
  "name": "cm-node",
  "version": "1.0.0",
  "description": "Simple Inventory Management System",
  "main": "dist/index.js",
  "scripts": {
    "clean": "npx rimraf ./dist",
    "build": "npm run clean && npx tsc",
    "start": "node ./dist/index.js",
    "start:dev": "ts-node-dev -r tsconfig-paths/register src/index.ts",
    "lint": "npx eslint src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run clean && npx tsc --project ./tsconfig.test.json && npm run test:e2e",
    "test:e2e": "npx jest --runInBand --detectOpenHandles --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eklikacz/cm-node-js.git"
  },
  "private": true,
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-validator": "^7.2.0",
    "http-status-codes": "^2.3.0",
    "js-yaml": "^4.1.0",
    "module-alias": "^2.2.3",
    "mongoose": "^8.6.2",
    "reflect-metadata": "^0.2.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/faker": "^5.5.9",
    "@types/jest": "^29.5.13",
    "@types/js-yaml": "^4.0.9",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^22.5.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-recommended": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "faker": "^5.5.3",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.0.1",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.6.0"
  },
  "_moduleAliases": {
    "@": "dist",
    "@common": "dist/module/common",
    "@order": "dist/module/order",
    "@product": "dist/module/product"
  }
}
